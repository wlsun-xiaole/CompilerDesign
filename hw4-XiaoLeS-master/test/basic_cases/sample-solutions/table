1: //&T-
2: table;
3: 
4: var bool : boolean;
5: var str : string;
6: 
7: funcWith0Parameters()
8: begin
9:     // constant
10:     var int1, int2 : 10;
11:     var str : "Gimme Gimme Gimme!!";
12:     var bool : true;
13:     var float : 2.56;
14:     var scientific : 111.111E-3;
15:     var octal : 0777;
16: end
17: end
18: 
19: funcWith1Parameter(p1 : integer)
20: begin
21:     var arr1 : array 1922 of array 1 of integer;
22: end
23: end
24: 
25: funcWith2Parameters(p1 : real; p2 : array 2 of real) : real
26: begin
27:     var constant : 19.25;
28: end
29: end
30: 
31: begin
32:     for i := 19 to 22 do
33:     begin
34:         var level3 : 3;
35: 
36:         begin
37:             var level4 : 4;
38:         end
39:     end
40:     end do
41: end
42: end
