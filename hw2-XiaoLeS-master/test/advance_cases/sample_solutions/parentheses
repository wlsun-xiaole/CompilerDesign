1: /**
2:  * parentheses.p: simple parentheses
3:  */
4: //&T-
5: parentheses;
6: 
7: begin
8:     var a, b: integer;
9:     
10:     a := 1+2*(3+4);
11:     a := (1+2)*3-4;
12:     a := (1+2)*(3-4);
13:     a := (1+3)*(12+34)/(49/7);
14:     a := b+a*(31-8);
15:     a := (a+b)*1;
16: end
17: end

|--------------------------------|
|  There is no syntactic error!  |
|--------------------------------|
